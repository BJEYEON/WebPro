<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bmiCalculatorMan" class="com.lec.ch02.ex2_bmi.BMICalculator">
		<property name="lowWeight" value="18.5"></property>
		<property name="normal" value="23.5"></property>
		<property name="overWeight" value="27.5"></property>
		<property name="obesity" value="32.5"></property>
	</bean>
	<bean id="man" class="com.lec.ch02.ex2_bmi.MyInfo">
		<property name="name" value="홍길동"/>
		<property name="height" value="175"/>
		<property name="weight" value="65"/>
		<property name="hobby">
			<list>
				<value>게임</value><value>수면</value><value>독서</value>
			</list>
		</property>
		<property name="bmiCalculator" ref="bmiCalculatorMan"></property>
	</bean>
</beans>
