<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="family" class="com.lec.ch03.Family" p:sisterName="홍딸램">
		<constructor-arg value="홍아빠"/>
		<constructor-arg value="김엄마"/>
		<property name="brotherName" value="홍아들"/>
	</bean>
	<bean id="student3" class="com.lec.ch03.Student">
		<constructor-arg value="성춘향"/>
		<constructor-arg value="20"/>
		<constructor-arg>
			<list>
				<value>그네타기</value><value>윳놀이</value>
			</list>
		</constructor-arg>
		<property name="weight" value="50"/>
	</bean>
</beans>
